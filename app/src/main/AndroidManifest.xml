<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cml.androiddevelopertools">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImageLoaderActivity" />
        <activity android:name=".NavigationActivity"></activity>
        <activity android:name=".navigation.RadioGroupViewPagerActivity" />
        <activity android:name=".navigation.FrameLayoutFragmentTabHostActivity" />
        <activity android:name=".navigation.CustomNavigationActivity" />
    </application>

</manifest>